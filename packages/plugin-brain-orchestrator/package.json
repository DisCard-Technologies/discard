{
  "name": "@discard/plugin-brain-orchestrator",
  "version": "1.0.0",
  "description": "elizaOS plugin for DisCard Brain Orchestrator - TEE-based intent parsing, planning, and orchestration",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "build": "tsup src/index.ts src/server.ts --format esm --dts --clean",
    "dev": "tsup src/index.ts src/server.ts --format esm --dts --watch",
    "start": "node dist/server.js",
    "start:plugin": "node dist/index.js",
    "proto:generate": "grpc_tools_node_protoc --plugin=protoc-gen-ts_proto=./node_modules/.bin/protoc-gen-ts_proto --ts_proto_out=./src/grpc/generated --ts_proto_opt=outputServices=grpc-js,esModuleInterop=true -I./src/grpc/proto ./src/grpc/proto/*.proto",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src/",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "elizaos",
    "plugin",
    "discard",
    "orchestrator",
    "brain",
    "tee",
    "phala",
    "nlp",
    "intent-parsing"
  ],
  "author": "DisCard Technologies",
  "license": "MIT",
  "dependencies": {
    "@elizaos/core": "^0.1.7-alpha.2",
    "@grpc/grpc-js": "^1.12.5",
    "@grpc/proto-loader": "^0.7.13",
    "openai": "^4.77.0",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@types/node": "^22.13.10",
    "@types/uuid": "^10.0.0",
    "grpc-tools": "^1.12.4",
    "ts-proto": "^2.6.1",
    "tsup": "^8.4.0",
    "typescript": "^5.8.3",
    "vitest": "^3.1.1"
  },
  "peerDependencies": {
    "@elizaos/core": ">=0.1.0"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "src/grpc/proto"
  ]
}
