{
  "extends": ["../../.eslintrc.json", "expo"],
  "env": {
    "react-native/react-native": true
  },
  "parserOptions": {
    "project": "./tsconfig.json"
  },
  "plugins": ["react-native"],
  "rules": {
    "react/react-in-jsx-scope": "off",
    "@typescript-eslint/no-unused-vars": ["error", { "argsIgnorePattern": "^_" }]
  }
}