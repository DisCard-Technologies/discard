# Stripe
STRIPE_SECRET_KEY=sk_test_fake_stripe_key
STRIPE_WEBHOOK_SECRET=whsec_test_fake_webhook_secret

# API Configuration
API_PORT=3001
API_HOST=localhost
RATE_LIMIT_WINDOW_MS=60000
RATE_LIMIT_MAX_REQUESTS=100

# Security
CORS_ORIGIN=http://localhost:3000
SESSION_SECRET=test-session-secret-for-testing

# Fraud Detection
FRAUD_DETECTION_ENABLED=true
FRAUD_VELOCITY_WINDOW_HOURS=24
FRAUD_MAX_TRANSACTIONS_PER_WINDOW=50
FRAUD_MAX_AMOUNT_PER_TRANSACTION=10000
FRAUD_HIGH_RISK_THRESHOLD=80

# Compliance
