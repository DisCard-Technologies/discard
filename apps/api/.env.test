COMPLIANCE_KYC_REQUIRED=false
COMPLIANCE_TRANSACTION_MONITORING_ENABLED=true

# Privacy
PRIVACY_DATA_RETENTION_DAYS=7
PRIVACY_ANONYMIZE_AFTER_DAYS=30
PRIVACY_DIFFERENTIAL_PRIVACY_EPSILON=1.0

# Testing
TEST_MODE=true
MOCK_EXTERNAL_APIS=true
TEST_USER_ID=test-user-123
TEST_CARD_ID=test-card-456

# Logging
LOG_LEVEL=error
LOG_FORMAT=json