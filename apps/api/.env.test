JWT_REFRESH_EXPIRES_IN=7d

# Encryption Keys
CARD_ENCRYPTION_KEY=test-card-encryption-key-32-chars
DELETION_SIGNING_KEY=test-deletion-signing-key-32-char
PAN_ENCRYPTION_KEY=test-pan-encryption-key-32-chars!
WALLET_ENCRYPTION_KEY=test-wallet-encryption-key-32-ch

# Marqeta
MARQETA_BASE_URL=https://sandbox-api.marqeta.com/v3
MARQETA_APPLICATION_TOKEN=test_app_token
